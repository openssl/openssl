{%- for sig in config['sigs'] %}
    {%- for variant in sig['variants'] %}
    {EVP_PKEY_{{ variant['name']|upper }}, SSL_a{{ variant['name']|upper }}}, /* SSL_PKEY_{{ variant['name']|upper }} */
        {%- for classical_alg in variant['mix_with'] %}
    {EVP_PKEY_{{ classical_alg['name']|upper }}_{{ variant['name']|upper }}, SSL_a{{ classical_alg['name']|upper }}{{ variant['name']|upper }}}, /* SSL_PKEY_{{ classical_alg['name']|upper }}_{{ variant['name']|upper }} */
        {%- endfor %}
    {%- endfor %}
{%- endfor %}

