    OSSL_URI_REFERENCE *profileUri;
    ASN1_OBJECT *targetOid;
    OSSL_URI_REFERENCE *targetUri;
} OSSL_COMMON_CRITERIA_MEASURES;

DECLARE_ASN1_FUNCTIONS(OSSL_COMMON_CRITERIA_MEASURES)

