pipeline {
  agent any
  stages {
    stage('asdf') {
      parallel {
        stage('asdf') {
          steps {
            retry(count: 8) {
              sh 'echo 789'
            }
            
          }
        }
        stage('dzcxzxc') {
          steps {
            sleep 1
          }
        }
        stage('zxc') {
          steps {
            echo 'ttt'
          }
        }
      }
    }
    stage('sdfsad') {
      parallel {
        stage('sdfsad') {
          steps {
            input 'asdadawd'
          }
        }
        stage('llll') {
          steps {
            archiveArtifacts(artifacts: 'wer', allowEmptyArchive: true)
          }
        }
      }
    }
  }
  environment {
    abc = '123'
    tctc = 'tfctfc'
  }
}